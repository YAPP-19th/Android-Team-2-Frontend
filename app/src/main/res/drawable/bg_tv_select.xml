<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="30dp" />
            <padding
                android:bottom="12dp"
                android:left="16dp"
                android:top="11dp" />
            <stroke
                android:width="2dp"
                android:color="@color/main_orange" />
        </shape>
    </item>
    <item
        android:width="24dp"
        android:height="24dp"
        android:drawable="@drawable/ic_select"
        android:end="10dp"
        android:gravity="end" />
</layer-list>